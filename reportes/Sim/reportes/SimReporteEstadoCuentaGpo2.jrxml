<jasperReport
		 name="SimReporteEstadoCuentaGpo2"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Portrait"
		 pageWidth="800"
		 pageHeight="802"
		 columnWidth="600"
		 columnSpacing="0"
		 leftMargin="0"
