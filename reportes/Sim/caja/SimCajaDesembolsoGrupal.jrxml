	<queryString><![CDATA[SELECT 
T.CVE_GPO_EMPRESA, 
T.CVE_EMPRESA, 
T.ID_MOVIMIENTO_OPERACION, 
T.ID_CLIENTE, 
T.ID_GRUPO, 
G.NOM_GRUPO, 
T.NUM_CICLO, 
T.ID_PRESTAMO, 
B.NOM_COMPLETO, 
-T.MONTO MONTO, 
CANTIDADES_LETRAS(MONTO) MONTO_AUTORIZADO_LETRAS, 
NVL(P.FECHA_ENTREGA,P.FECHA_REAL) FECHA,
TO_CHAR(SYSDATE, 'HH:MI AM') AS HORA,
T.ID_SUCURSAL, 
A.NOM_SUCURSAL, 
T.ID_CAJA, 
D.CALLE||' '||D.NUMERO_INT||' '||'COL.'||' '||D.NOM_ASENTAMIENTO||' '||'C.P.'||' '||D.CODIGO_POSTAL||','||' '||D.NOM_DELEGACION||','||' '||D.NOM_ESTADO DOMICILIO 
FROM SIM_CAJA_TRANSACCION T, 
SIM_PRESTAMO P, 
SIM_SUCURSAL_CAJA S, 
RS_GRAL_PERSONA B, 
SIM_CAT_SUCURSAL A, 
RS_GRAL_DOMICILIO D, 
SIM_GRUPO G 
WHERE T.CVE_GPO_EMPRESA = 'SIM' 
AND T.CVE_EMPRESA = 'CREDICONFIA' 
AND T.ID_TRANSACCION_GRUPO = '62' 
AND T.CVE_MOVIMIENTO_CAJA = 'DESGPO' 
AND S.CVE_GPO_EMPRESA = T.CVE_GPO_EMPRESA 
AND S.CVE_EMPRESA = T.CVE_EMPRESA 
AND S.ID_SUCURSAL = T.ID_SUCURSAL 
AND S.ID_SUCURSAL||'-'||S.ID_CAJA = '5-1' 
AND P.CVE_GPO_EMPRESA (+)= T.CVE_GPO_EMPRESA 
AND P.CVE_EMPRESA (+)= T.CVE_EMPRESA 
AND P.ID_PRESTAMO (+)= T.ID_PRESTAMO 
AND B.CVE_GPO_EMPRESA (+)= T.CVE_GPO_EMPRESA 
AND B.CVE_EMPRESA (+)= T.CVE_EMPRESA 
AND B.ID_PERSONA (+)= T.ID_CLIENTE 
AND A.CVE_GPO_EMPRESA = T.CVE_GPO_EMPRESA 
AND A.CVE_EMPRESA = T.CVE_EMPRESA 
AND A.ID_SUCURSAL = T.ID_SUCURSAL 
AND D.CVE_GPO_EMPRESA (+)= A.CVE_GPO_EMPRESA 
AND D.CVE_EMPRESA (+)= A.CVE_EMPRESA 
AND D.ID_DOMICILIO (+)= A.ID_DIRECCION 
AND G.CVE_GPO_EMPRESA (+)= T.CVE_GPO_EMPRESA 
AND G.CVE_EMPRESA (+)= T.CVE_EMPRESA 
AND G.ID_GRUPO (+)= T.ID_GRUPO]]></queryString>

